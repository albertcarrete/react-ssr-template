  "main": "index.js",
  "scripts": {
    "lint:eslint": "eslint --ext=.js --ext=.jsx source/client",
    "start": "concurrently --kill-others \"npm run start:proxy\" \"npm run start:dev\"",
    "start:dev": "webpack-dev-server --open --mode development",
    "start:proxy": "nodemon source/server/server.config.dev.js",
    "start:prod": "npm run build:dist && cross-env NODE_ENV=production node dist/server/server.config.prod.js",
    "build:dist": "npm run build:assets && rm -rf dist/ && babel source/ -d dist/",
    "build:assets": "cross-env NODE_ENV=production webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "node public/assets/bundle.js"
  },
